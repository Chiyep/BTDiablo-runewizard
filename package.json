    "@typescript-eslint/eslint-plugin": "^4.33.x",
    "@typescript-eslint/parser": "^4.33.x",
    "@vitejs/plugin-vue": "^2.2.2",
    "autoprefixer": "^10.4.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
